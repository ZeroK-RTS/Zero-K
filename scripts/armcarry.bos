Killed(severity, corpsetype)
{
	if (severity <= 25)
	{
		corpsetype = 1;
	}
	if (severity <= 50)
	{
		corpsetype = 1;
	}
	if (severity <= 99)
	{
		corpsetype = 2;
	}
	corpsetype = 2;
}
