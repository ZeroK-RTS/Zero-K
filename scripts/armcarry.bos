		}
	}
	if( Static_Var_6 == 1 )
	{
		turn hatch1 to x-axis <-90.000000> speed <180.000000>;
		wait-for-turn hatch1 around x-axis;
	}
	if( Static_Var_6 == 2 )
	{
		turn hatch2 to x-axis <-90.000000> speed <180.000000>;
		wait-for-turn hatch2 around x-axis;
	}
	if( Static_Var_6 == 3 )
